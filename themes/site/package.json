{
  "name": "speedcubesite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/**/*.{js,vue}"
    ],
    "exclude": [
      "node_modules/**/*.js",
      "tests/**/*.{js,vue}"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bedard-cube": "^1.0.0-beta.0",
    "bezier-easing": "^2.1.0",
    "chart.js": "^2.7.3",
    "color": "^3.1.0",
    "focus-trap": "^4.0.2",
    "lodash-es": "^4.17.11",
    "moment": "^2.22.2",
    "no-scroll": "^2.1.1",
    "pluralize": "^7.0.0",
    "portal-vue": "^1.5.1",
    "spyfu-vue-functional": "^0.2.6",
    "spyfu-vue-utils": "^0.2.0",
    "spyfu-vuex-helpers": "^0.12.4",
    "three": "^0.101.1",
    "vue": "^2.5.17",
    "vue-chartjs": "^3.4.0",
    "vue-color": "^2.7.0",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^2.8.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.1.2",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "glob-all": "^3.1.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma-chai-dom": "^1.1.0",
    "karma-coverage": "^1.1.2",
    "karma-json-reporter": "^1.2.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-spec-reporter": "^0.0.32",
    "node-sass": "^4.9.0",
    "nyc": "^13.1.0",
    "postcss-import": "^12.0.0",
    "postcss-url": "^8.0.0",
    "purgecss-webpack-plugin": "^1.3.1",
    "sass-loader": "^7.0.1",
    "sinon": "^7.0.0",
    "sinon-chai": "^3.2.0",
    "spyfu-vue-factory": "^0.7.4",
    "tailwindcss": "^0.7.0",
    "vue-cli-plugin-moment": "^0.1.1",
    "vue-cli-plugin-unit-karma": "^0.5.0",
    "vue-cli-plugin-vuetober": "^3.0.0-rc.1",
    "vue-template-compiler": "^2.5.17"
  }
}
